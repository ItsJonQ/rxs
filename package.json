{
  "name": "reactive-css",
  "version": "0.0.3",
  "description": "Reactive CSS: Super fast dynamic CSS rules.",
  "main": "dist/reactive-css.js",
  "scripts": {
    "build:banner": "node ./scripts/banner.js",
    "build:minify": "uglifyjs src/reactive-css.js --output dist/reactive-css.min.js",
    "build:copy": "cp src/reactive-css.js dist/reactive-css.js",
    "build": "npm run build:copy && npm run build:minify && npm run build:banner",
    "test": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsJonQ/reactive-css.git"
  },
  "author": "ItsJonQ <itsjonq@gmail.com> (http://jonquach.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsJonQ/reactive-css/issues"
  },
  "homepage": "https://github.com/ItsJonQ/reactive-css#readme",
  "devDependencies": {
    "uglify-js": "2.8.21"
  }
}
